{
  "name": "easyimg",
  "version": "1.0.0",
  "description": "🖼️ Easy and professional image management system with authentication and API",
  "author": {
    "name": "Yaroslav",
    "url": "https://t.me/Calabalac"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:swc": "nest build --builder swc",
    "build:fast": "nest build --builder swc --type-check",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:swc": "nest start --watch --builder swc",
    "start:api": "nest start --watch --builder swc",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run start:api\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:swc && npm run build:frontend",
    "deploy:local": "concurrently \"npm run start:api\" \"npm run dev:frontend\" --names \"API,Frontend\" --prefix-colors \"blue,green\"",
    "deploy:local:bg": "concurrently \"npm run start:api\" \"npm run dev:frontend\" --names \"API,Frontend\" --prefix-colors \"blue,green\" > logs/deploy.log 2>&1 &",
    "stop:local": "pkill -f \"nest start\" && pkill -f \"vite\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.0",
    "daisyui": "^5.0.43",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [
    "image-management",
    "nestjs",
    "supabase",
    "authentication",
    "api",
    "upload",
    "gallery",
    "admin-panel",
    "react",
    "vite",
    "spa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Calabalac/easyimg.git"
  },
  "bugs": {
    "url": "https://github.com/Calabalac/easyimg/issues"
  },
  "homepage": "https://github.com/Calabalac/easyimg#readme"
}
